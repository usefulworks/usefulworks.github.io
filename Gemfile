source "https://rubygems.org"

# Hello! This is where you manage which Jekyll version is used to run.
# When you want to use a different version, change it below, save the
# file and run `bundle install`. Run Jekyll with `bundle exec`, like so:
#
#     bundle exec jekyll serve
#
# This will help ensure the proper Jekyll version is running.
# gem "jekyll", "~> 4.3.2"

# This is the default theme for new Jekyll sites. You may change this to anything you like.
# gem "minima", "~> 2.5"

# If you want to use GitHub Pages, remove the "gem "jekyll"" above and
# uncomment the line below. To upgrade, run `bundle update github-pages`.
# GitHub Pages deps versions are here https://pages.github.com/versions/
gem "github-pages", "~> 232", group: :jekyll_plugins

# plugin gems - those listed here are loaded irrespective of site configuration
group :jekyll_plugins do
    gem "jekyll-seo-tag", "~> 2.8.0"
    gem "jekyll-sitemap", "~> 1.4.0"
    gem "jekyll-coffeescript", "~> 1.2.2"
    gem "jekyll-default-layout", "~> 0.1.5"
    gem "jekyll-gist", "~> 1.5.0"
    gem "jekyll-github-metadata", "~> 2.16.1"
    gem "jekyll-optional-front-matter", "~> 0.3.2"
    gem "jekyll-paginate", "~> 1.1.0"
    gem "jekyll-readme-index", "~> 0.3.0"
    gem "jekyll-titles-from-headings", "~> 0.5.3"
    gem "jekyll-relative-links", "~> 0.6.1"
end
